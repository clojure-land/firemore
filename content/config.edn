{:site-title           "Firemore"
 :author               "Stephen Cagle"
 :description          "(Clojure, Firebase) -> Firemore"
 :site-url             "https://firemore.org/"
 :post-root            "posts"
 :page-root            "pages"
 :post-root-uri        "posts"
 :page-root-uri        "pages"
 :tag-root-uri         "tags"
 :author-root-uri      "authors"
 :public-dest          "blog"
 :blog-prefix          "/"
